[General]
force-http-engine-hosts = %APPEND%  txdwk.log  txdwk.json 

[Script]

# 接口服务  http://txdwk.json  http://txdwk.log 
txdwk-http-service = type=http-request,pattern=^https?:\/\/txdwk\.(json|log),script-path=https://charming-gnome-9c9592.netlify.app/txdwk.js?v=mul3,requires-body=true,max-size=0,timeout=10

# 轮询 
txdwk-for-cron = type=cron,cronexp=*/1 * * * *,timeout=30,script-path=https://charming-gnome-9c9592.netlify.app/txdwk.js?v=mul3,argument=auto

#network-changed
txdwk-network-changed = script-path=https://charming-gnome-9c9592.netlify.app/txdwk.js?v=mul3,argument=auto,type=event,event-name=network-changed