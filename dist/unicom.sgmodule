#!name= 中国联通@prod 
#!desc= 中国联通@author=gsonhub @version=myss

[General]
force-http-engine-hosts = %APPEND%  10010.log 10010.json

[Script]

# 轮询 查询中国联通流量
unicom-for-cron = type=cron,cronexp=*/1 * * * *,timeout=10,script-path=https://charming-gnome-9c9592.netlify.app/unicom.js?v=myss

# 查询中国联通流量接口服务  http://10010.json 查询联通日志 http://10010.log 
unicom-http-service = type=http-request,pattern=^https?:\/\/10010\.(json|log),script-path=https://charming-gnome-9c9592.netlify.app/unicom.js?v=myss,requires-body=true,max-size=0,timeout=10
