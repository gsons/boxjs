#!name= gsonhub@prod的脚本v=avxz
#!desc= 筋斗云定时签到，查询筋斗云个人信息接口服务v=avxz

[MITM] 
hostname = %APPEND% www.somersaultcloud.top,www.somersaultcloud.xyz,www.pinganbaiyun.cn,*.gdbs.gov.cn

[General]
force-http-engine-hosts = %APPEND% cloud.json cloud.log 10010.json 10010.log pingan.log  10000.log 

[Script]

# 轮询 查询流量
10010-for-cron = type=cron,cronexp=*/1 * * * * ?,timeout=10,script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/10010.js?v=avxz

# 查询联通流量接口服务  http://10010.json 查询联通日志 http://10010.log 
10010-http-service = type=http-request,pattern=^https?:\/\/10010\.(json|log),script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/10010.js?v=avxz,requires-body=true,max-size=0,timeout=10


# 筋斗云获取cookie
cloud = type=http-request,pattern=^https?:\/\/(my\.|)somersaultcloud\.(xyz|top)\/user$,script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/cloud.js?v=avxz,timeout=10

# 筋斗云定时签到
cloud-cron = type=cron,cronexp=0 0 * * *,timeout=30,script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/cloud.js?v=avxz

# 查询筋斗云个人信息接口服务 http://cloud.json  查询筋斗云运行日志 http://cloud.log
cloud-service = type=http-request,pattern=^https?:\/\/cloud\.(json|log),script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/cloud.js?v=avxz,requires-body=true,max-size=0,timeout=10

# 平安白云 
pingan-service = type=http-response,pattern=^https?:\/\/(www\.|)pinganbaiyun\.(cn|com)\/baiyunuser\/entranceguard\/getList$,script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/pingan.js?v=avxz,requires-body=true,max-size=0

# 查询平安白云日志 http://pingan.log 
pingan-http-service = type=http-request,pattern=^https?:\/\/pingan\.log,script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/pingan.js?v=avxz,requires-body=true,max-size=0,timeout=10


# 查询中国电信日志 http://10000.log 
10000-http-service = type=http-request,pattern=^https?:\/\/10000\.log,script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/10000.js?v=avxz,requires-body=true,max-size=0,timeout=10


